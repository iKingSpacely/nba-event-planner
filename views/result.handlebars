<script src="/js/result.js"></script>


{{#each teamGames}}
  <div class="accordion accordion-flush" id="resultsPopulate">
    <div class="accordion-item">
      <h2 class="accordion-header">
        <button class="accordion-button collapsed" type="button" data-bs-toggle="collapse" data-bs-target="#{{GlobalGameID}}" aria-expanded="false" aria-controls="{{GlobalGameID}}">
          {{HomeTeam}} vs {{AwayTeam}}
        </button>
      </h2>
      <div id="{{GlobalGameID}}" class="accordion-collapse collapse" data-bs-parent="#resultsPopulate">
        <div class="accordion-body">
          Home Team: {{HomeTeam}}<br>
          Home Score: {{HomeTeamScore}}<br>
          Away Team: {{AwayTeam}}<br>
          Away Score: {{AwayTeamScore}}<br>
          Date: {{format_date DateTime}}<br>
        </div>
      </div>
    </div>
  </div>
{{/each}}


<script src="/js/result.js"></script>

